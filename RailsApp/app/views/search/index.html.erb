<h1>Search#index</h1>
<p>Find me in app/views/search/index.html.erb</p>


<%= form_tag("/search/results", method: "get") do %>

  <p>
  <%= label_tag(:coursecode, "Course Code:") %>
  <%= text_field_tag :coursecode %>
  </p>

  <p>
  <%= label_tag(:coursename , "Course Name:") %>
  <%= text_field_tag :coursename %>
  </p>

  <p>
  <%= label_tag(:instructor , "Instructor:") %>
  <%= text_field_tag :instructor %>
  </p>

  <p>
  <%= label_tag(:avail_seats , "Available Seats:") %>
  <%= number_field_tag :avail_seats, nil, min: 0 %>
  </p>
  
  <p>
  <%= label_tag(:crn, "CRN:") %>
  <%= number_field_tag :crn, nil, in: 10000...99999 %>
  </p>

  <%= submit_tag 'Search Classes' %>

<% end %>
